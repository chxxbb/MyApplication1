<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f4f4f4">

    <RelativeLayout
        android:id="@+id/my_reservation_page_title_relativelayout"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y88"
        android:layout_alignParentTop="true"
        android:background="#3496d3">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="预约详情"
            android:textColor="@android:color/white"
            android:textSize="@dimen/x36" />

        <ImageView
            android:id="@+id/my_reservation_page_exit"
            android:layout_width="@dimen/x32"
            android:layout_height="@dimen/x32"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/x40"
            android:src="@mipmap/registered_exit" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/my_reservation_page_prompt_relativelayout"
        android:layout_width="match_parent"
        android:layout_height="@dimen/y60"
        android:layout_below="@id/my_reservation_page_title_relativelayout"
        android:background="#ffefd7">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="请凭订单号去前台取号"
            android:textColor="#ff9600"
            android:textSize="@dimen/y24" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/my_reservation_page_top_relativelayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/my_reservation_page_prompt_relativelayout"
        android:background="@android:color/white"
        android:paddingBottom="@dimen/y30"
        android:paddingLeft="@dimen/x40"
        android:paddingRight="@dimen/x40">

        <TextView
            android:id="@+id/my_reservation_page_hospital_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/y36"
            android:text="深圳天使儿童医院"
            android:textColor="#323232"
            android:textSize="@dimen/y30" />

        <TextView
            android:id="@+id/my_reservation_page_department_title_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/my_reservation_page_hospital_text"
            android:layout_marginTop="@dimen/y36"
            android:text="科室 : "
            android:textColor="#999999"
            android:textSize="@dimen/y26" />

        <TextView
            android:id="@+id/my_reservation_page_department_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@id/my_reservation_page_department_title_text"
            android:layout_marginLeft="@dimen/x10"
            android:layout_toRightOf="@id/my_reservation_page_department_title_text"
            android:text="小儿外科"
            android:textColor="#999999"
            android:textSize="@dimen/y26" />

        <TextView
            android:id="@+id/my_reservation_page_doctor_title_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/my_reservation_page_department_title_text"
            android:layout_marginTop="@dimen/y30"
            android:text="医生 : "
            android:textColor="#999999"
            android:textSize="@dimen/y26" />

        <TextView
            android:id="@+id/my_reservation_page_doctor_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@id/my_reservation_page_doctor_title_text"
            android:layout_marginLeft="@dimen/x10"
            android:layout_toRightOf="@id/my_reservation_page_doctor_title_text"
            android:text="柳下飞"
            android:textColor="#999999"
            android:textSize="@dimen/y26" />

        <TextView
            android:id="@+id/my_reservation_page_name_title_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/my_reservation_page_doctor_title_text"
            android:layout_marginTop="@dimen/y30"
            android:text="姓名 : "
            android:textColor="#999999"
            android:textSize="@dimen/y26" />

        <TextView
            android:id="@+id/my_reservation_page_name_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@id/my_reservation_page_name_title_text"
            android:layout_marginLeft="@dimen/x10"
            android:layout_toRightOf="@id/my_reservation_page_name_title_text"
            android:text="二营长"
            android:textColor="#999999"
            android:textSize="@dimen/y26" />

        <TextView
            android:id="@+id/my_reservation_page_time_title_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/my_reservation_page_name_title_text"
            android:layout_marginTop="@dimen/y30"
            android:text="日期 : "
            android:textColor="#999999"
            android:textSize="@dimen/y26" />

        <TextView
            android:id="@+id/my_reservation_page_time_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@id/my_reservation_page_time_title_text"
            android:layout_marginLeft="@dimen/x10"
            android:layout_toRightOf="@id/my_reservation_page_time_title_text"
            android:text="2015-7-5"
            android:textColor="#999999"
            android:textSize="@dimen/y26" />

        <TextView
            android:id="@+id/my_reservation_page_address_title_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/my_reservation_page_time_title_text"
            android:layout_marginTop="@dimen/y30"
            android:text="地址 : "
            android:textColor="#999999"
            android:textSize="@dimen/y26" />

        <TextView
            android:id="@+id/my_reservation_page_address_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@id/my_reservation_page_address_title_text"
            android:layout_marginLeft="@dimen/x10"
            android:layout_toRightOf="@id/my_reservation_page_address_title_text"
            android:text="深圳市龙华新区工业西路97号"
            android:textColor="#999999"
            android:textSize="@dimen/y26" />


    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/my_reservation_page_top_relativelayout"
        android:layout_marginTop="@dimen/y30"
        android:background="@android:color/white"
        android:paddingBottom="@dimen/y30"
        android:paddingLeft="@dimen/x40"
        android:paddingRight="@dimen/x40">

        <TextView
            android:id="@+id/my_reservation_page_botton_title_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/y36"
            android:text="预约挂号订单"
            android:textColor="#323232"
            android:textSize="@dimen/y30" />

        <TextView
            android:id="@+id/my_reservation_page_botton_number_title_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/my_reservation_page_botton_title_text"
            android:layout_marginTop="@dimen/y36"
            android:text="订单号 : "
            android:textColor="#999999"
            android:textSize="@dimen/y26" />

        <TextView
            android:id="@+id/my_reservation_page_botton_number_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@id/my_reservation_page_botton_number_title_text"
            android:layout_marginLeft="@dimen/x10"
            android:layout_toRightOf="@id/my_reservation_page_botton_number_title_text"
            android:text="123456789630258"
            android:textColor="#999999"
            android:textSize="@dimen/y26" />

        <TextView
            android:id="@+id/my_reservation_page_botton_time_title_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/my_reservation_page_botton_number_title_text"
            android:layout_marginTop="@dimen/y30"
            android:text="有效期 : "
            android:textColor="#999999"
            android:textSize="@dimen/y26" />

        <TextView
            android:id="@+id/my_reservation_page_botton_time_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@id/my_reservation_page_botton_time_title_text"
            android:layout_marginLeft="@dimen/x10"
            android:layout_toRightOf="@id/my_reservation_page_botton_time_title_text"
            android:text="2015-7-20"
            android:textColor="#999999"
            android:textSize="@dimen/y26" />

        <Button
            android:layout_width="@dimen/x136"
            android:layout_height="@dimen/y50"
            android:layout_alignBottom="@id/my_reservation_page_botton_time_title_text"
            android:layout_alignParentRight="true"
            android:background="@drawable/my_reservation_page_button_shape"
            android:text="取消预约"
            android:textColor="@android:color/white"
            android:textSize="@dimen/y22" />


    </RelativeLayout>


</RelativeLayout>